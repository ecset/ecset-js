{
  "name": "elsinore",
  "version": "4.2.1",
  "description": "a javascript Entity Component System",
  "keywords": [
    "elsinore",
    "entity",
    "entity-component",
    "ecs",
    "data-driven"
  ],
  "author": {
    "name": "Alexander Veenendaal",
    "email": "alex@opendoorgonorth.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/odogono/elsinore-js",
  "bugs": {
    "url": "https://github.com/odogono/elsinore-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/odogono/elsinore-js.git"
  },
  "main": "lib/index.js",
  "jsnext:main": "dist/elsinore.es2015.js",
  "dependencies": {
    "deep-equal": "^1.0.0",
    "odgn-backbone-model": "^1.3.9",
    "odgn-bitfield": "^0.9.0",
    "underscore": "^1.8.2"
  },
  "non-dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-es2015-classes": "^6.18.0",
    "babel-plugin-transform-es2015-destructuring": "^6.19.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babelify": "^7.3.0",
    "del": "^2.2.0",
    "envify": "^3.4.0",
    "event-stream": "^3.3.2",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-flow": "^1.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "shelljs": "^0.6.0",
    "sinon": "~1.17.3",
    "tap-dot": "^1.0.5",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "unreachable-branch-transform": "^0.5.1",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "^1.0.0"
  },
  "directories": {
    "lib": "lib",
    "dist": "dist"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:es": "rollup -c rollup.config.es.js",
    "build:prod": "rollup -c rollup.config.prod.js",
    "compile:es6": "babel src --out-dir es6 --plugins=transform-inline-environment-variables,transform-es2015-modules-commonjs,transform-flow-strip-types,transform-es2015-destructuring,transform-object-rest-spread,transform-async-to-generator",
    "compile": "babel src --out-dir lib --plugins=transform-inline-environment-variables,transform-es2015-classes,transform-flow-strip-types,transform-es2015-destructuring,transform-object-rest-spread,transform-async-to-generator",
    "clean": "rm -rf node_modules build dist lib; mkdir build; npm install",
    "dist:noclean": "npm run compile && npm run compile:es6 && npm run build && npm run build:prod && npm run build:es",
    "dist": "npm run dist:noclean",
    "prepublish": "npm run dist",
    "test": "NODE_ENV=test babel-node test/index.js --plugins=transform-inline-environment-variables,transform-es2015-modules-commonjs,transform-flow-strip-types,transform-es2015-destructuring,transform-object-rest-spread,transform-async-to-generator | tap-dot",
    "spec": "NODE_ENV=test babel-node test/index.js --plugins=transform-inline-environment-variables,transform-es2015-modules-commonjs,transform-flow-strip-types,transform-es2015-destructuring,transform-object-rest-spread,transform-async-to-generator | tap-spec"
  }
}
