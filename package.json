{
  "name": "elsinore",
  "version": "5.2.2",
  "description": "a javascript Entity Component System",
  "keywords": [
    "elsinore",
    "entity",
    "entity-component",
    "ecs",
    "data-driven"
  ],
  "author": {
    "name": "Alexander Veenendaal",
    "email": "alex@opendoorgonorth.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/odogono/elsinore-js",
  "bugs": {
    "url": "https://github.com/odogono/elsinore-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/odogono/elsinore-js.git"
  },
  "main": "dist/elsinore.cjs.js",
  "module": "dist/elsinore.esm.js",
  "browser": "dist/elsinore.umd.js",
  "dependencies": {
    "minivents": "^2.2.0",
    "odgn-bitfield": "^1.0.3",
    "pull-pushable": "2.2.0"
  },
  "non-dependencies": {},
  "devDependencies": {
    "@types/jest": "24.0.6",
    "@types/tape": "4.2.33",
    "jest": "24.1.0",
    "odgn-json": "^0.3.1",
    "prettier": "1.16.4",
    "pull-file": "^1.1.0",
    "pull-stream": "3.6.9",
    "pull-stringify": "^2.0.0",
    "pull-utf8-decoder": "^1.0.2",
    "rollup": "1.2.2",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "2.1.0",
    "rollup-plugin-terser": "4.0.4",
    "rollup-plugin-typescript": "1.0.0",
    "stream-to-pull-stream": "^1.7.2",
    "tap-dot": "2.0.0",
    "tape": "4.10.1",
    "terser": "3.16.1",
    "ts-jest": "24.0.0",
    "ts-node": "8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3"
  },
  "scripts": {
    "build": "rollup -c",
    "compile": "tsc",
    "clean": "rm -rf node_modules build dist lib; mkdir build; npm install",
    "dist:noclean:old": "NODE_ENV=production npm run compile && npm run compile:es2015 && npm run build && npm run build:es",
    "dist:noclean": "NODE_ENV=production npm run compile && npm run build",
    "dist": "NODE_ENV=production npm run dist:noclean",
    "lint": "eslint  --ext .ts src",
    "prepare": "npm run dist",
    "test": "NODE_ENV=test ts-node test/index.js | tap-dot",
    "spec": "NODE_ENV=test ts-node test/index.js | tap-spec"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(ts)$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node"
  }
}
