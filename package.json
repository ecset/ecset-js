{
  "name": "elsinore",
  "version": "4.5.1",
  "description": "a javascript Entity Component System",
  "keywords": [
    "elsinore",
    "entity",
    "entity-component",
    "ecs",
    "data-driven"
  ],
  "author": {
    "name": "Alexander Veenendaal",
    "email": "alex@opendoorgonorth.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/odogono/elsinore-js",
  "bugs": {
    "url": "https://github.com/odogono/elsinore-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/odogono/elsinore-js.git"
  },
  "main": "dist/elsinore.js",
  "jsnext:main": "dist/elsinore.module.js",
  "module": "dist/elsinore.module.js",
  "dependencies": {
    "odgn-backbone-model": "^1.3.9",
    "odgn-bitfield": "^0.9.1",
    "pull-pushable": "2.1.1",
    "underscore": "^1.8.2"
  },
  "non-dependencies": {},
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-classes": "^6.23.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.22.0",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-es2015-rollup": "3.0.0",
    "eslint": "^3.16.1",
    "eslint-plugin-import": "2.3.0",
    "pull-stream": "3.6.0",
    "rollup": "^0.45.1",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-commonjs": "8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1",
    "sinon": "2.1.0",
    "tap-dot": "^1.0.5",
    "tap-spec": "^4.1.1",
    "tape": "4.6.3",
    "uglify-js": "github:mishoo/UglifyJS2#harmony"
  },
  "directories": {
    "es2015": "es2015",
    "lib": "lib",
    "dist": "dist"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:es": "rollup -c rollup.config.es.js",
    "compile": "babel src --out-dir lib --plugins=transform-es2015-shorthand-properties,transform-inline-environment-variables,transform-es2015-modules-commonjs,transform-es2015-destructuring,transform-object-rest-spread",
    "compile:es2015": "babel src --out-dir es2015 --plugins=transform-es2015-shorthand-properties,transform-inline-environment-variables,transform-object-rest-spread",
    "clean": "rm -rf node_modules build dist lib; mkdir build; npm install",
    "dist:noclean": "NODE_ENV=production npm run compile && npm run compile:es2015 && npm run build && npm run build:es",
    "dist": "npm run dist:noclean",
    "lint": "./node_modules/.bin/eslint ./src --ext .js",
    "lint-fix": "./node_modules/.bin/eslint --fix ./src --ext .js",
    "prepublish": "npm run dist",
    "test": "NODE_ENV=test babel-node test/index.js --plugins=transform-inline-environment-variables,transform-es2015-modules-commonjs,transform-object-rest-spread | tap-dot",
    "spec": "NODE_ENV=test babel-node test/index.js --plugins=transform-inline-environment-variables,transform-es2015-modules-commonjs,transform-object-rest-spread | tap-spec"
  }
}
