{
  "name": "elsinore",
  "version": "5.1.10",
  "description": "a javascript Entity Component System",
  "keywords": [
    "elsinore",
    "entity",
    "entity-component",
    "ecs",
    "data-driven"
  ],
  "author": {
    "name": "Alexander Veenendaal",
    "email": "alex@opendoorgonorth.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/odogono/elsinore-js",
  "bugs": {
    "url": "https://github.com/odogono/elsinore-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/odogono/elsinore-js.git"
  },
  "main": "dist/elsinore.cjs.js",
  "module-old": "./lib",
  "module": "dist/elsinore.esm.js",
  "browser": "dist/elsinore.umd.js",
  "dependencies": {
    "odgn-bitfield": "^0.9.3",
    "pull-pushable": "2.2.0"
  },
  "non-dependencies": {},
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "8.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.23.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.22.0",
    "babel-plugin-transform-inline-environment-variables": "0.4.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015-rollup": "3.0.0",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.11.0",
    "pull-stream": "3.6.8",
    "rollup": "0.58.2",
    "rollup-plugin-babel": "3.0.4",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-uglify": "3.0.0",
    "sinon": "5.0.7",
    "tap-dot": "^1.0.5",
    "tap-spec": "^4.1.1",
    "tape": "4.9.0",
    "uglify-es": "3.3.10",
    "underscore": "1.9.0"
  },
  "scripts": {
    "build": "rollup -c",
    "build:old": "rollup -c rollup.config.js",
    "build:es": "rollup -c rollup.config.es.js",
    "compile:old": "babel src --out-dir lib --plugins=transform-es2015-shorthand-properties,transform-inline-environment-variables,transform-es2015-modules-commonjs,transform-es2015-destructuring,transform-object-rest-spread",
    "compile:es2015": "babel src --out-dir es2015 --plugins=transform-es2015-shorthand-properties,transform-inline-environment-variables,transform-object-rest-spread",
    "compile:esm": "BABEL_ENV=build-esm babel src --out-dir lib",
    "clean": "rm -rf node_modules build dist lib; mkdir build; npm install",
    "dist:noclean:old": "NODE_ENV=production npm run compile:esm && npm run compile:es2015 && npm run build && npm run build:es",
    "dist:noclean": "NODE_ENV=production npm run compile:esm && npm run build",
    "dist": "npm run dist:noclean",
    "lint": "./node_modules/.bin/eslint ./src --ext .js",
    "lint-fix": "./node_modules/.bin/eslint --fix ./src --ext .js",
    "prepare": "npm run dist",
    "test": "NODE_ENV=test babel-node test/index.js --plugins=transform-inline-environment-variables,transform-es2015-modules-commonjs,transform-object-rest-spread,transform-class-properties | tap-dot",
    "spec": "NODE_ENV=test babel-node test/index.js --plugins=transform-inline-environment-variables,transform-es2015-modules-commonjs,transform-object-rest-spread,transform-class-properties | tap-spec"
  }
}
