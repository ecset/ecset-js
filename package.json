    "odgn-json": "^0.3.1",
    "prettier": "^1.14.2",
    "pull-file": "^1.1.0",
    "pull-stream": "3.6.9",
    "pull-stringify": "^2.0.0",
    "pull-utf8-decoder": "^1.0.2",
