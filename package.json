{
  "name": "elsinore",
  "version": "4.2.5",
  "description": "a javascript Entity Component System",
  "keywords": [
    "elsinore",
    "entity",
    "entity-component",
    "ecs",
    "data-driven"
  ],
  "author": {
    "name": "Alexander Veenendaal",
    "email": "alex@opendoorgonorth.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/odogono/elsinore-js",
  "bugs": {
    "url": "https://github.com/odogono/elsinore-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/odogono/elsinore-js.git"
  },
  "main": "dist/elsinore.js",
  "jsnext:main": "dist/elsinore.es2015.js",
  "dependencies": {
    "deep-equal": "^1.0.0",
    "odgn-backbone-model": "^1.3.9",
    "odgn-bitfield": "^0.9.0",
    "underscore": "^1.8.2"
  },
  "non-dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-classes": "^6.23.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-es2015-rollup": "3.0.0",
    "eslint": "^3.16.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "event-stream": "^3.3.2",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-commonjs": "7.0.0",
    "rollup-plugin-flow": "1.1.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "shelljs": "0.7.6",
    "sinon": "1.17.7",
    "tap-dot": "^1.0.5",
    "tap-spec": "^4.1.1",
    "tape": "4.6.3"
  },
  "directories": {
    "es2015": "es2015",
    "lib": "lib",
    "dist": "dist"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:es": "rollup -c rollup.config.es.js",
    "build:prod": "rollup -c rollup.config.prod.js",
    "compile": "babel src --out-dir lib --plugins=transform-es2015-shorthand-properties,transform-inline-environment-variables,transform-es2015-modules-commonjs,transform-flow-strip-types,transform-es2015-destructuring,transform-object-rest-spread",
    "compile:es2015": "babel src --out-dir es2015 --plugins=transform-es2015-shorthand-properties,transform-inline-environment-variables,transform-flow-strip-types,transform-object-rest-spread",
    "clean": "rm -rf node_modules build dist lib; mkdir build; npm install",
    "dist:noclean": "npm run compile && npm run compile:es2015 && npm run build && npm run build:prod && npm run build:es",
    "dist": "npm run dist:noclean",
    "lint": "./node_modules/.bin/eslint ./src --ext .js",
    "lint-fix": "./node_modules/.bin/eslint --fix ./src --ext .js",
    "prepublish": "npm run dist",
    "test": "NODE_ENV=test babel-node test/index.js --plugins=transform-inline-environment-variables,transform-es2015-modules-commonjs,transform-flow-strip-types | tap-dot",
    "spec": "NODE_ENV=test babel-node test/index.js --plugins=transform-inline-environment-variables,transform-es2015-modules-commonjs,transform-flow-strip-types | tap-spec"
  }
}
