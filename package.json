  },
  "main": "dist/elsinore.cjs.js",
  "module": "dist/elsinore.esm.js",
  "browser": "dist/elsinore.umd.js",
  "dependencies": {
    "odgn-bitfield": "^1.0.3",
    "pull-pushable": "2.2.0"
  },
  "non-dependencies": {},
  "devDependencies": {
    "@types/tape": "^4.2.32",
    "@types/underscore": "^1.8.9",
    "eslint": "^5.4.0",
    "eslint-config-typescript": "^1.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
