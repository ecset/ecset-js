  "scripts": {
    "build": "rollup -c",
    "compile": "tsc",
    "clean": "rm -rf node_modules build dist lib; mkdir build; npm install",
    "dist:noclean:old": "NODE_ENV=production npm run compile && npm run compile:es2015 && npm run build && npm run build:es",
    "dist:noclean": "NODE_ENV=production npm run compile && npm run build",
    "dist": "NODE_ENV=production npm run dist:noclean",
    "lint": "eslint  --ext .ts src",
    "prepare": "npm run dist",
    "test": "NODE_ENV=test ts-node test/index.js --plugins=transform-inline-environment-variables,transform-es2015-modules-commonjs,transform-object-rest-spread,transform-class-properties | tap-dot",
    "spec": "NODE_ENV=test ts-node test/index.js --plugins=transform-inline-environment-variables,transform-es2015-modules-commonjs,transform-object-rest-spread,transform-class-properties | tap-spec"
  }
}
